---
const { tweetLink } = Astro.props

interface Props {
  tweetLink: string
}
---

<blockquote
  class="twitter-tweet"
  data-dnt="true"
  data-theme="dark"
  data-align="center"
  data-conversation="none"
>
  <a href={`https://twitter.com/${tweetLink}?ref_src=twsrc%5Etfw`}
    >Loading tweet&hellip;</a
  >
</blockquote>
<script async src="https://platform.twitter.com/widgets.js" charset="utf-8"
></script>
